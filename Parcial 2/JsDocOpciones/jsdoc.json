{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [ "./src" ],
        "exclude": [ ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/braintree-jsdoc-template",
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": true
    }
}